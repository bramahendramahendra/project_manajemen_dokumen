NEXT_PUBLIC_REFRESH_CHECK_INTERVAL=8      # 8 menit untuk development

# =============================================================
# ENVIRONMENT SETTINGS
# =============================================================
NODE_ENV=development

# Debug mode (opsional)
NEXT_PUBLIC_DEBUG_MODE=true